    <meta name="description" content="This is meta description">
    <meta name="author" content="Themefisher">
    <link rel="shortcut icon" href="images/favicon.png" type="image/x-icon">
    <link rel="icon" href="images/favicon.png" type="image/x-icon">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/PrimewareAssets/source/plugins/slick/slick.css">
    <link href="~/PrimewareAssets/source/plugins/font-awesome/fontawesome.min.css" rel="stylesheet" />
    <link href="~/PrimewareAssets/source/plugins/font-awesome/brands.css" rel="stylesheet" />
    <link href="~/PrimewareAssets/source/plugins/font-awesome/solid.css" rel="stylesheet" />

